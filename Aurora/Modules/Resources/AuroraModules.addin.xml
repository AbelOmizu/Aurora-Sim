<Addin id="Aurora.Modules" version="0.2">
    <Runtime>
        <Import assembly="Aurora.Modules.dll"/>
    </Runtime>

    <Dependencies>
        <Addin id="OpenSim" version="0.5" />
    </Dependencies>

    <Extension path = "/OpenSim/RegionModules">
      <RegionModule id="IWCEntityTransferModule"           type="OpenSim.Region.CoreModules.Framework.EntityTransfer.IWCEntityTransferModule" />
      <RegionModule id="AuroraWorldMapModule"           type="Aurora.Modules.AuroraWorldMapModule" />
      <RegionModule id="IWCSimulationConnectorModule"           type="Aurora.Modules.IWCSimulationConnectorModule" />
      <RegionModule id="AuroraOfflineMessageModule" type="Aurora.Modules.AuroraOfflineMessageModule" />
      <RegionModule id="Auth"           type="Aurora.Modules.Auth" />
      <RegionModule id="AbuseReports"           type="Aurora.Modules.AbuseReports" />
	  <RegionModule id="AuroraProfileModule"           type="Aurora.Modules.AuroraProfileModule" />
	  <RegionModule id="GodModifiers"           type="Aurora.Modules.GodModifiers" />
	  <RegionModule id="LureModule"           type="Aurora.Modules.LureModule" />
	  <RegionModule id="EstateSettingsModule"           type="Aurora.Modules.EstateSettingsModule" />
    </Extension>
</Addin>
