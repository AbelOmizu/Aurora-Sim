<Addin id="Aurora.Modules" version="0.2">
    <Runtime>
        <Import assembly="Aurora.Modules.dll"/>
    </Runtime>

    <Dependencies>
        <Addin id="OpenSim" version="0.5" />
    </Dependencies>

    <Extension path = "/OpenSim/RegionModules">
		<RegionModule id="IWComms"           type="Aurora.Modules.IWComms" />
		<RegionModule id="AuroraWorldMapModule"           type="Aurora.Modules.AuroraWorldMapModule" />
		<RegionModule id="AuroraOfflineMessageModule" type="Aurora.Modules.AuroraOfflineMessageModule" />
		<RegionModule id="AuroraChatModule"           type="Aurora.Modules.AuroraChatModule" />
		<RegionModule id="AbuseReportsModule"           type="Aurora.Modules.AbuseReportsModule" />
		<RegionModule id="AuroraSearchModule"           type="Aurora.Modules.AuroraSearchModule" />
		<RegionModule id="AuroraProfileModule"           type="Aurora.Modules.AuroraProfileModule" />
		<RegionModule id="GodModifiers"           type="Aurora.Modules.GodModifiers" />
		<RegionModule id="LureModule"           type="Aurora.Modules.LureModule" />
		<RegionModule id="EstateSettingsModule"           type="Aurora.Modules.EstateSettingsModule" />
		<RegionModule id="SimProtection"           type="Aurora.Modules.SimProtection" />
		<RegionModule id="ObjectCacheModule"           type="Aurora.Modules.ObjectCacheModule" />
		<RegionModule id="CallingCardModule"           type="Aurora.Modules.CallingCardModule" />
		<RegionModule id="PresenceModule"           type="Aurora.Modules.PresenceModule" />
		<RegionModule id="CrossRegionBanSystem"           type="Aurora.Modules.CrossRegionBanSystem" />
		<RegionModule id="DisplayNamesModule"           type="Aurora.Modules.DisplayNamesModule" />
		<RegionModule id="BannedViewersModule"           type="Aurora.Modules.BannedViewersModule" />
		<RegionModule id="VisitorLoggerModule"           type="Aurora.Modules.VisitorLoggerModule" />
	</Extension>
	<Extension path = "/OpenSim/Startup">
		<Plugin id="UpdaterPlugin" type="Aurora.Modules.UpdaterPlugin" />
	</Extension>
	<Extension path = "/Aurora/ChatPlugins">
		<ChatPlugins id="TranslatorPlugin"           type="Aurora.Modules.Avatar.AuroraChat.TranslatorPlugin" />
		<ChatPlugins id="CalcChatPlugin"           type="Aurora.Modules.Avatar.AuroraChat.CalcChatPlugin" />
		<ChatPlugins id="AdminChatPlugin"           type="Aurora.Modules.Avatar.AuroraChat.AdminChatPlugin" />
	</Extension>
</Addin>
