<Addin id="Aurora.ScriptEngine.AuroraDotNetEngine" version="0.2">
    <Runtime>
        <Import assembly="Aurora.ScriptEngine.AuroraDotNetEngine.dll"/>
    </Runtime>

    <Dependencies>
        <Addin id="OpenSim" version="0.5" />
    </Dependencies>

    <Extension path = "/OpenSim/RegionModules">
        <RegionModule id="AuroraDotNetEngine" type="Aurora.ScriptEngine.AuroraDotNetEngine.ScriptEngine" />
    </Extension>

	<Extension path = "/OpenSim/ScriptPlugins">
		<ScriptPlugin id="LS_Api"             type="Aurora.ScriptEngine.AuroraDotNetEngine.APIs.LS_Api" />
		<ScriptPlugin id="LSL_Api"            type="Aurora.ScriptEngine.AuroraDotNetEngine.APIs.LSL_Api" />
		<ScriptPlugin id="MOD_Api"            type="Aurora.ScriptEngine.AuroraDotNetEngine.APIs.MOD_Api" />
		<ScriptPlugin id="AA_Api"             type="Aurora.ScriptEngine.AuroraDotNetEngine.APIs.AA_Api" />
		<ScriptPlugin id="OSSL_Api"           type="Aurora.ScriptEngine.AuroraDotNetEngine.APIs.OSSL_Api" />
	</Extension>
</Addin>
